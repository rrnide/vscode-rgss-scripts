{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "preLaunchTask": "npm: compile",
      "sourceMaps": false,
      "outFiles": ["${workspaceFolder}/extension.js", "!**/node_modules/**"]
    }
  ]
}
