{
  "files": ["./extension.ts"],
  "compilerOptions": {
    "strict": true,
    "target": "ES6",
    "moduleResolution": "Node",
    "module": "CommonJS",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["ES6"],
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "useUnknownInCatchVariables": false
  }
}
